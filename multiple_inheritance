package Com.sample;

//Parent class
class Person1 {
String name;
int age;

void showPerson() {
   System.out.println("Name: " + name);
   System.out.println("Age: " + age);
}
}

//Child class
class Employee1 extends Person1 {
int salary;

void showEmployee() {
   showPerson();
   System.out.println("Salary: " + salary);
}
}

class Manager1 extends Employee1 {
String pos;

void showManager() {
   showEmployee();
   System.out.println("Job Role: " + pos);
}
}

//Main class
public class MultiLevelInheritence {
	public static void main(String[] args) {
   Manager1 m = new Manager1();

   m.name = "navya";
   m.age = 18;
   m.salary = 100000;
   m.pos = "Software Developer";

   m.showManager();
 }
}

Output :
Name: navya
Age: 18
Salary: 100000
Job Role: Software Developer
